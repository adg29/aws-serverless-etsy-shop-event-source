{
  "name": "aws-serverless-etsy-shop-event-source",
  "version": "1.0.0",
  "description": "A serverless app that periodically polls the public Etsy API and invokes a given lambda function to process changes to the active listings for a specific shop.",
  "main": "index.js",
  "author": "William Weiss <william@codification.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "preversion": "yarn test",
    "version": "yarn build && git add -A .",
    "postversion": "git push && git push --tags",
    "clean": "rimraf \"dist/**\"",
    "test": "tape -r ts-node/register src/**/*.spec.ts",
    "build": "yarn clean && tsc && yarn test"
  },
  "dependencies": {
    "got": "^8.3.0",
    "limiter": "^1.1.3",
    "md5": "^2.2.1",
    "rxjs": "^5.5.10",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.1",
    "@types/dotenv": "^4.0.3",
    "@types/got": "^8.3.0",
    "@types/node": "^9.6.6",
    "@types/tape": "^4.2.32",
    "@types/winston": "^2.3.9",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.226.1",
    "dotenv": "^5.0.1",
    "rimraf": "^2.6.2",
    "tape": "^4.9.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}
